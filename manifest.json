{
    "name": "ebb",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "Chrome extension to mitigate on-screen movement.",
    "icons": {
        "16": "icons/placeholder16x16.png",
        "32": "icons/placeholder32x32.png",
        "48": "icons/placeholder48x48.png",
        "128": "icons/placeholder128x128.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["lib/jquery.min.js","content-scripts/content.js"]
        }
    ]
}
